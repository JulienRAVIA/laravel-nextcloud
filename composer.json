{
    "name": "xylis/laravel-nextcloud",
    "type": "library",
    "version": "1.1.0",
    "description": "Laravel 5 NextCloud Filesystem | Supports Laravel 8",
    "keywords": [
        "jedlikowski",
        "laravel-nextcloud"
    ],
    "homepage": "https://github.com/Julien RAVIA/laravel-nextcloud",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Jedlikowski",
            "role": "Developer"
        },
        {
            "name": "Julien Ravia",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "~5.6|^7.0",
        "illuminate/filesystem": "~5.0|~6.0|~7.0|~8.0",
        "league/flysystem-webdav": "^1.0.8",
        "jakeasmith/http_build_url": "^1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "~5.0|~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Jedlikowski\\NextCloudStorage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jedlikowski\\NextCloudStorage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jedlikowski\\NextCloudStorage\\NextCloudServiceProvider"
            ]
        }
    }
}
